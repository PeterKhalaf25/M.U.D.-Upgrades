console.WriteLine("Hello World");

//Player Class

Class Player
{
    int moveSpd = 100;
    int atkDmg = 100;
    int atkSpd = 100;
    int abilityHaste = 100;
    int abilityDmg = 100;
    int dodgeDistance = 100;

    public void moveSpdBuff(float modifier)
    {
        moveSpd = modifier*moveSpd;
    }

    public void atkDmgBuff(float modifier)
    {
        atkDamage = atkDamage*modifier;
    }

    public void atkSpdBuff(float modifier)
    {
        atkSpd = atkSpd*100;
    }

    public void hasteBuff(float modifier)
    {
        abilityHaste = abilityHaste*modifier;
    }

    public void abilityDmgBuff(float modifier)
    {
        abilityDmg = abilityDmg*modifier;
    }

    public void dodgeBuff(float modifier)
    {
        dodgeDistance = dodgeDistance*modifier
    }
}